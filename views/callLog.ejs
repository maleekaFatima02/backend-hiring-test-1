
<table class="table table-striped table-primary">
    <thead>
      <tr>
        <th>From</th>
        <th>Duration</th>
        <th>Status</th>
        <th>VoiceMail</th>
        <th>Time</th>
      </tr>
    </thead>
    <tbody>
      <%if (calls.length >0){ %>
      <% calls.forEach(function(row){ %>
      <tr>
        <td><%= row.from %></td>
        <td><%= row.duration %></td>
        <td><%= row.status %></td>
        <%if (row.voiceMail){ %>
        <td><a href="<%= row.voiceMailURL %>">Voice Mail</a></td>
        <% }else{ %>
            <td></td>
        <% } %>
        <td><%= moment(row.createdAt).format('h:mm:ss a') %></td>
      </tr>
      <% })}else{ %>
        <tr>
            <td colspan="6">No Call</td>
            
          </tr>
        <% } %>
    </tbody>
  </table>
